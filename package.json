  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "express-fileupload": "^1.2.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "react-cookies": "^0.1.1",
    "react-iframe": "^1.8.0",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "web-vitals": "^1.0.1"
  }
}
